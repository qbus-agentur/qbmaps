<T3DataStructure>
	<sheets>
		<sDEF>
			<ROOT>
				<TCEforms>
					<sheetTitle>Karteneinstellungen</sheetTitle>
				</TCEforms>
				<type>array</type>
				<el>
					<settings.coordinates>
						<section>1</section>
						<type>array</type>
						<el>
							<item>
								<type>array</type>
								<title>Marker</title>
								<el>
									<address>
										<TCEforms>
											<exclude>1</exclude>
											<label>Adresse</label>
											<config>
												<type>input</type>
												<eval>trim</eval>
												<size>30</size>
											</config>
										</TCEforms>
									</address>
									<coordinate>
										<TCEforms>
											<exclude>1</exclude>
											<label>Google Koordinaten (alternativ)</label>
											<config>
												<type>input</type>
												<eval>trim</eval>
												<size>30</size>
											</config>
										</TCEforms>
									</coordinate>
									<description>
										<TCEforms>
											<exclude>1</exclude>
											<label>Beschreibung</label>
											<config>
												<type>text</type>
												<rows>5</rows>
												<cols>30</cols>
												<eval>trim</eval>
											</config>
											<defaultExtras>richtext[]:rte_transform[mode=ts_css]</defaultExtras>
										</TCEforms>
									</description>
								</el>
							</item>
						</el>
					</settings.coordinates>
				</el>
			</ROOT>
		</sDEF>
	</sheets>
</T3DataStructure>
